<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Phone Number</th>
      <th>Email</th>
      <th>Role</th>

      <th>Organization</th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td> 
          <%= user.name %> 
        </td>
        <td> 
          <%= user.title %> 
        </td>
        <td> 
          <%= user.mobile %> 
        </td>
        <td> 
          <%= user.email %> 
        </td>
        <td>
            <%= user.role %>
        </td>
        <td> 
          <%= user.legacy_organization.name %> 
        </td>
    <% end %>
  </tbody>
</table>